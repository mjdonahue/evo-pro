  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "src-tauri/src/**/*.rs": [
      "cd src-tauri && cargo fmt --"
