{
  "name": "evo-pro",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:hot:frontend dev:hot:backend",
    "dev:legacy": "vite",
    "dev:hot": "npm-run-all --parallel dev:hot:frontend dev:hot:backend",
    "dev:hot:frontend": "vite",
    "dev:hot:backend": "cd src-tauri && cargo watch -w src -x 'run --features dev'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "docs": "typedoc",
    "test": "npm-run-all --sequential test:unit test:integration",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:integration:coverage": "vitest run --coverage --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "test:performance:watch": "vitest --config vitest.performance.config.ts",
    "test:all": "npm-run-all --sequential test:unit test:integration test:e2e test:performance",
    "coverage": "npm-run-all --sequential coverage:unit coverage:integration coverage:merge",
    "coverage:unit": "vitest run --coverage",
    "coverage:integration": "vitest run --coverage --config vitest.integration.config.ts",
    "coverage:merge": "node scripts/merge-coverage.js",
    "coverage:open": "open coverage/combined/index.html",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "rust:check": "cd src-tauri && cargo check",
    "rust:clippy": "cd src-tauri && cargo clippy -- -D warnings",
    "rust:format": "cd src-tauri && cargo fmt --all",
    "rust:test": "cd src-tauri && cargo test",
    "check": "npm-run-all --parallel lint format:check rust:check rust:clippy",
    "fix": "npm-run-all --sequential lint:fix format rust:format",
    "analyze": "npx eslint-find-rules --unused .eslintrc.cjs && npx eslint --max-warnings=0 .",
    "complexity": "npx ts-complexity --max-complexity 15 --patterns \"src/**/*.{ts,tsx}\" --exclude \"**/*.test.{ts,tsx}\"",
    "quality": "npm-run-all --parallel analyze complexity",
    "quality:metrics": "node scripts/code-quality-metrics.js",
    "quality:report": "npm-run-all quality:metrics && open code-quality-reports/report.html",
    "security:verify": "./scripts/verify-security-tools.sh",
    "security:check": "npm-run-all lint rust:clippy && pnpm audit --audit-level=moderate",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
